{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "#/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "types": ["node"],
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,

    "skipLibCheck": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules/", "dist"]
}
