{
  "name": "@king-3/file-kit",
  "version": "1.1.0",
  "packageManager": "pnpm@10.25.0",
  "description": "多功能文件工具箱（Base64 转换 · 视频转音频 · 加密/解密）",
  "type": "module",
  "keywords": [
    "file",
    "base64",
    "video",
    "audio",
    "ffmpeg",
    "crypto",
    "encrypt",
    "decrypt"
  ],
  "license": "MIT",
  "homepage": "https://github.com/coderking3/file-kit#readme",
  "bugs": {
    "url": "https://github.com/coderking3/file-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderking3/file-kit"
  },
  "author": "king3 <king3.wm@gmail.com>",
  "files": [
    "bin",
    "dist"
  ],
  "imports": {
    "#/*": "./src/*"
  },
  "bin": {
    "fkt": "./bin/cli.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "fkt": "node bin/cli.js",
    "dev": "tsx src/cli.ts",
    "build": "tsx scripts/build.ts",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "release": "bumpp"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "ansis": "^4.2.0",
    "citty": "^0.1.6",
    "execa": "^9.6.1"
  },
  "devDependencies": {
    "@king-3/eslint-config": "^4.4.0",
    "@king-3/prettier-config": "^3.4.0",
    "@types/node": "^20.19.26",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "tsdown": "^0.18.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
